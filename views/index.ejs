  <link rel="stylesheet" href="/stylesheets/style.css" />
  <style>
    body {
      font-family: Arial, sans-serif;
      background-color: #f5f5f5;
      margin: 0;
      padding: 0;
    }

    .container {
      max-width: 800px;
      margin: 0 auto;
      padding: 20px;
    }

    h1 {
      color: #333;
      text-align: center;
      margin-top: 50px;
    }

    p {
      color: #666;
      text-align: center;
      font-size: 18px;
      margin-top: 20px;
    }

    .image-container {
      display: flex;
      justify-content: center;
      margin-top: 50px;
    }

    .image-container img {
      width: 300px;
      height: auto;
      border-radius: 10px;
      margin: 10px;
    }

    .slider {
      width: 100%;
      height: auto;
      overflow: hidden;
      position: relative;
      margin: 0 auto;
      margin-top: 50px;
      border-radius: 10px;
    }

    .slider img {
      width: 100%;
      height: 200px;
      /* Cambiar el valor para ajustar el tamaño vertical */
      
    }

    .slider-container {
      display: flex;
      width: 300%;
      /* Cambiar el valor para ajustar el tamaño horizontal */
      transition: transform 0.5s ease-in-out;
      animation: slide 5s infinite;
      border-radius: 10px;
    }

    .slide {
      flex: 0 0 33.33%;
      /* Cambiar el valor para ajustar el tamaño horizontal */
    }

    @keyframes slide {
      0% {
        transform: translateX(0);
      }

      25% {
        transform: translateX(-33.33%);
        /* Cambiar el valor para ajustar el tamaño horizontal */
      }

      50% {
        transform: translateX(-66.66%);
        /* Cambiar el valor para ajustar el tamaño horizontal */
      }

      75% {
        transform: translateX(-100%);
        /* Cambiar el valor para ajustar el tamaño horizontal */
      }

      100% {
        transform: translateX(0);
      }
    }
  </style>
</head>

<body>
  <div class="container">
    <h1>
      <%= title %>
    </h1>
    <p>Bienvenido a la práctica de GitHub Actions <%= title %>
    </p>
